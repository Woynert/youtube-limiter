{
  "manifest_version": 2,
  "name": "Youtube Limiter",
  "version": "1.1.0",
  "description": "Youtube Limiter",
  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": ["storage", "scripting", "<all_urls>"],

  "background": {
    "scripts": ["main.js"],
    "persistent": true
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "youtube-limiter@woynert.com"
    }
  }
}
